<?xml version="1.0" encoding="UTF-8"?><unload unload_date="2018-03-16 14:59:01" unload_format="1" unload_table="ops_unv_tmplt" unload_version="6.4.3.0"><ops_unv_tmplt action="INSERT_OR_UPDATE"><credentials_var/><broadcast_cluster/><agent/><use_common_script>false</use_common_script><credentials/><icon_filesize>4802</icon_filesize><icon></icon><icon_date_created>2016-09-13 12:17:26</icon_date_created><description>UT running AL_RWJobLauncher.EXE</description><sys_updated_on>2018-03-16 14:55:03</sys_updated_on><agent_var/><icon_filename>bods.png</icon_filename><sys_id>808d3dc1638b468c8871473c73094af9</sys_id><output_return_text/><sys_updated_by>ops.admin</sys_updated_by><variable_prefix>var</variable_prefix><sys_created_on>2016-09-13 12:00:23</sys_created_on><script_windows>#!/usr/local/bin/python2.7

###################################################
#
#    Name: run_sapbods.py
#
#    Purpose: Run an SAP BODS Job
#             using AL_RWJobLauncher.exe
#             
#             
#
#
#    Prerequisites:
#     - Universal Agent for Windows/Linux
#     - Python 2.7.x
#     - Requires the Python modules
#     - datetime, re, glob, sys, time, datetime
#     - Note: module can be added using pip
#     - e.g. use: pip install re
#
#    Disclaimer
#     No support and no warranty are provided for this Universal Task. 
#     The use of this document and the related Universal Task is on your own risk.
#     Before using this task in a production system, please perform extensive testing.
#     Stonebranch GmbH assumes no liability for damage caused by 
#     the performance of the Universal Tasks
#
#     Version History:
#     0.1  Nils Buer 2017-12-03 Initial Version
#     0.2  Nils Buer 2017-12-18 Logging added
#     0.3  Nils Buer 2018-02-16 Log level as UT Para
#     0.4  Nils Buer 2018-02-20 removed argparse for UT
#     0.5  Nils Buer 2018-03-16 more error handling added     
####################################################
version = "0.5"
# Used modules

import re
import glob
import sys
import datetime
import os 
import time
import logging

# -- logging settings DEBUG, INFO, WARNING, ERROR, CRITICAL

logging.basicConfig(level=logging.${ops_var_loglevel}, format=' %(asctime)s - %(levelname)s - %(message)s')

# Sample CLI Command to test in Command Shell or dos Box


def main():
    Set_Variables()                              # -- set variables
    Check_server()                               # -- check the server an port for connection
    Test_Dir(rwjldir)                            # -- test is directories exist
    Test_Dir(jobdir)                             # -- test is directories exist
    Test_Dir(server_log_path)                    # -- test is directories exist
    Test_Dir(tracedir)                           # -- test is directories exist
    Run_Job()                                    # -- start an SAPBODS Job
    Gettrace_Log()                               # -- retrieves the log file
    Geterror_Log()                               # -- retrieve error log 


# -- Set Variables from Universal Task Form or Command Line Input
def Set_Variables():
    logging.info("Executing version " + version + " with the following paramaters")   
    logging.info('####### Parameters:') # print out all parameters
    logging.info(r'server_log_path: ${ops_var_server_log_path}')
    logging.info(r'jobdir: ${ops_var_jobdir}') 
    logging.info(r'tracedir: ${ops_var_tracedir}')
    logging.info(r'rwjldir: ${ops_var_rwjldir}')
    logging.info('parameters: ${ops_var_parameters}')
    logging.info('poll: ${ops_var_poll}')
    logging.info('host: ${ops_var_host}')
    logging.info('port: ${ops_var_port}')
    logging.info('jobname: ${ops_var_jobname}') 
    global jobname_file,tracedir,jobdir,server_log_path,rwjldir
    jobname_file = r'${ops_var_jobdir}\${ops_var_jobname}.txt'
    server_log_path = r'${ops_var_server_log_path}'
    rwjldir = r'${ops_var_rwjldir}'
    jobdir = r'${ops_var_jobdir}'
    tracedir = r'${ops_var_tracedir}'
    logging.info('jobname_file: ' + jobname_file) 

    
def Check_server():
    import socket
    s = socket.socket()
    address = '${ops_var_host}'
    port = ${ops_var_port}  # port number is a number, not string
    try:
        s.connect((address, port)) 
        # originally, it was 
        # except Exception, e: 
        # but this syntax is not supported anymore. 
        logging.debug(('connection with %s:%d. success') % (address, port))
    except Exception as e: 
        print("something's wrong with %s:%d. Exception is %s" % (address, port, e))
        logging.error(('something is wrong with %s:%d. Exception is %s') % (address, port, e)) 
        sys.exit(1)
    finally:
        s.close()

  
def Test_Dir(testdir):
    if os.path.isdir(testdir):
       logging.debug('Directory found ' + testdir)
    else:
       logging.error('Directory not found: ' + testdir)
       sys.exit(1) 


def Run_Job():

  # ## start an SAPBODS Job
  global start_time
  logging.info('##### Processing Start ##### ')
  start_time = time.time() # time when the SAPBO job is started
  logging.debug('Agent Home DIR: ' + os.getcwd())
  os.chdir(rwjldir)
  logging.debug('Dir changed to AL_RWJobLauncher Home DIR: ' + os.getcwd())
  #  os.system('AL_RWJobLauncher.exe "${ops_var_server_log_path}" ${ops_var_parameters} ${ops_var_poll} "inet:${ops_var_host}:${ops_var_port}" -C "${ops_var_jobdir}\${ops_var_jobname}.txt"')
  cmd = "AL_RWJobLauncher.exe " + '"' + server_log_path + '"' + " ${ops_var_parameters} ${ops_var_poll} " + '"' + "inet:${ops_var_host}:${ops_var_port}" + '" -C "' + jobname_file + '"'
  logging.debug ("cmd = " + cmd)
  os.system(cmd)

def Gettrace_Log():
  # directories
  # retrieve the guid form inside the jobnamefile &lt;jobname&gt;.txt
  f = open(os.path.join(jobname_file), "r")  
  text = f.read() 
  guid = re.findall(r'-G"(.*?)"', text)[0]
  logging.info('Collecting tracefile for job=${ops_var_jobname} ,with guid=' + str(guid))

  trace_file = str(tracedir +"\*trace*"+ str(guid) + '.txt')
  # trace_file = str("C:\ProgramData\SAP BusinessObjects\Data Services\log\JobServer_1\\bi4_cms_dsn__dba\*trace*" + str(guid) + '.txt')

  # collect all files related to the guid in the log folder and indentify the lastest one
  list_of_files = glob.glob(trace_file) # * means all if need specific format then *.csv
  latest_file = max(list_of_files, key=os.path.getctime)
  latest_file_ct = os.path.getctime(latest_file) # creation date of the tracefile
  if latest_file_ct &gt;= start_time:
    # print out latest trace file for the related guid to standard out
    logging.info('#### Tracefile: ' + latest_file)
    f = open(os.path.join(latest_file), "r")  
    text = f.read() 
    logging.info('##### Trace Log start: ')
    logging.info (text)
    logging.info('##### Trace Log End ##### ')
    logging.info('##### Processing Done ##### ')
  else:
    logging.error('No tracefile found ' + latest_file)
    sys.exit(1)    

def Geterror_Log():

  # retrieve the guid form inside the jobnamefile &lt;jobname&gt;.txt
  f = open(os.path.join(jobname_file), "r")  
  job_file = f.read() 
  guid = re.findall(r'-G"(.*?)"', job_file)[0]
  logging.debug('Collecting error log for job: ${ops_var_jobname},with guid=' + str(guid))
  error_file = trace_file = str(tracedir + "\*error*"+ str(guid) + '.txt')
  # error_file = str("C:\ProgramData\SAP BusinessObjects\Data Services\log\JobServer_1\\bi4_cms_dsn__dba\*error*" + str(guid) + '.txt')

  # collect all files related to the guid in the log folder and indentify the lastest one
  list_of_files = glob.glob(error_file) # * means all if need specific format then *.csv
  latest_file = max(list_of_files, key=os.path.getctime)
  latest_file_ct = os.path.getctime(latest_file) # creation date of the errorfile
  if latest_file_ct &gt;= start_time:
    # print out latest error file for the related guid to standard out
    # 
    f = open(os.path.join(latest_file), "r")  
    error_log = f.read()
    error_msg="error"
    if error_msg in error_log:
      logging.error('Error file: ' + latest_file)
      logging.error('##### Error Log start: ')
      logging.error(error_log)
      logging.error('##### Error Log End ##### ')
      sys.exit(1)
    else:
      logging.info('No error found in: ' + latest_file)
  else:
    logging.error('No errorfile found: ' + latest_file)
    sys.exit(1)  
# -- 

# -- Execute
main()
